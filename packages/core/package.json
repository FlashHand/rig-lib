{
  "name": "rig-lib",
  "version": "1.0.6",
  "description": "A foundation utility library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "yarn build",
    "publish:npm": "npm publish --registry=https://registry.npmjs.org/ --access public"
  },
  "dependencies": {
    "@rig-lib/semaphore": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/FlashHand/rig-lib.git"
  },
  "author": "WangBo <ralwayne@163.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
